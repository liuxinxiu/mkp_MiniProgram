/** toast.wxss **/
.wxToast_mask{width:100%;height:100%;position:fixed;left:0px;top:0px;z-index:10000;background:rgba(0,0,0,0);opacity:0;display:none}.wxToast_show{display:block}.wxToast_content{max-width:80%;min-width:90px;position:absolute;left:50%;top:20%;transform:translate3d(-50%,0,0);padding:15px;color:#fff;font-size:17px;text-align:center;border-radius:5px;background:rgba(0,0,0,.8)}.wxToast_img{width:55px;height:55px;display:block;margin:0 auto 8px auto}


/**layout.wxss**/
.page_bottom{ width: 100%; height: 50px; position: fixed; z-index: 100; left: 0; right: 0; bottom: 0; background: #fff; color: #131313; font-size: 14px;-moz-box-shadow:0px -3px 20px #E3E3E3; -webkit-box-shadow:0px -3px 20px #E3E3E3; box-shadow:0px -3px 20px #E3E3E3;}
.page_bottom .sbox{ width: 100%; height: 50px;}
.page_bottom .sbox .page_btn{ float: left; width: 50%; height: 50px; text-align: center; line-height: 50px;}
.page_bottom .sbox .btnl .inner{ 
   background-position: 38px 5px;
   background-size: 28%;
}
.page_bottom .sbox .btnl.on .inner{ 
   background-position: 40px -100px;
}


.page_bottom .sbox .btnr .inner{ 
   background-position: 24px -209px;
   background-size: 28%;
}
.page_bottom .sbox .btnr.on .inner{ 
   background-position: 24px -314px;
}


.page_bottom .sbox .liney{width: 1px; height: 30px; font-size: 0px; overflow: hidden; position: absolute; left: 50%; bottom: 10px; background: #e6e6e6;}
.page_bottom .sbox .page_btn.on{ color: #30d127;}

/* vertical-align */

.page_bottom .sbox .inner{ background-repeat: no-repeat; background-image: url(data:image/png;base64,) }



/****** slide-bottom add liuxinxiu 2018-07-23 ******/
.mask-layer{ position: fixed; width: 100%; height: 100%; z-index: 200; background: #000; top:0; right: 0; left: 0; bottom: 0; filter: alpha(opacity=40); -moz-opacity: 0.7; opacity: 0.7;}

.slide-bottom{ position: fixed; width: 100%; z-index: 201; height: 200px; bottom: 0px; background: #fff; transform: translateY(100%);}
.slide-bottom .title{height: 40px; border-bottom: 1px solid #ebebeb; display: flex;  flex-flow: row;}
.slide-bottom .title .sclose{ width: 40px; height: 40px; }
.slide-bottom .title .stbox{ line-height: 40px; flex:1;/*这里设置为占比1，填充满剩余空间*/  text-align: center;}
.slide-bottom .title .sconfirm{ width: 40px; height: 40px;}
.slide-bottom .sbox.stab{ padding-top: 10px;}
.slide-bottom .sbox.stab .ntab{ display:  inline-block; width: 20%; padding-top: 3px; padding-bottom: 3px; border: 1px solid #ebebeb; border-radius: 5px; margin: 0 8px; margin-bottom: 10px; text-align: center; }
.slide-bottom .sbox.stab .ntab.on{ background: #38d530; border: 1px solid #38d530; color: #fff;}

/****** slide-bottom liuxinxiu add 2018-07-23 ******/
@keyframes slidedown {
    from {
        transform: translateY(0);
    }
    to {
        transform: translateY(100%);
    }
}
.slidedown,.slide-bottom.slidedown {
    animation: slidedown 0.3s linear;
    transform: translateY(100%);
}
.slideup,.slide-bottom.slideup {
    animation: slideup 0.3s linear;
    transform: translateY(0);
}
@keyframes slideup {
    from {
        transform: translateY(100%);
    }
    to {
        transform: translateY(0);
    }
}

.slide-bottom .title .sconfirm{ background-image: url(data:image/png;base64,);
 background-repeat: no-repeat;
 background-position: center center;
 background-size: 30%;
}
.slide-bottom .title .sclose{ background-image: url(data:image/png;base64,); 
background-size: 26%;
background-repeat: no-repeat;
background-position: center center;

}

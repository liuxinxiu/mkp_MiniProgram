/* page/order/add_order_mainpage.wxss */

page {
   background-color:#f5f5f5;
   color:#131313;
   font-size:14px;
   font-family:-apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

/****** 订单订单提交页通用盒子 ******/
.orderbox{ background: #fff; padding: 10px 25px; /*min-height: 50px;*/ /*border-radius: 3px 3px 3px 3px;*/ margin-bottom: 10px;}
.orderbox .or-line{ background-color: #f3f3f3; height: 2px; font-size: 0; overflow: hidden; width: 86%; margin: 0 auto}
.orderbox .or-dbox { height: 1px; position: relative; z-index: 100; width: 100%; display: flex; width: 86%; margin: 0 auto}

.orderbox .flexWarp .lkey{ width: 30%;}
.orderbox .flexWarp .rcon{ width: 70%;} 

.bline{border-bottom: 1px solid #ebebeb;}
.acbox,.bdbox{ background: #fff; padding: 0px 25px; color: #a9a9a9;}
.acbox .flexWarp .tline,
.acbox .flexWarp .cline{border-bottom: 1px solid #ebebeb; width: 100%;}
.acbox .flexWarp .tline{}
.acbox .flexWarp .cline{}


.acbox .flexWarp{
  display:flex;/*设为伸缩容器*/  
  flex-flow:row;/*伸缩项目单行排列*/  
}
.acbox .flexWarp .fl{
   flex:1;/*这里设置为占比1，填充满剩余空间*/ 
   -webkit-box-flex: 1; 
}
.acbox .flexWarp .add-ico{
   width: 50px; height: 50px; background: url(data:image/png;base64,) no-repeat;
   background-size:30%;
   background-position:100% 15px; 
 }
 .acbox .flexWarp .add-ico .n{ 
   width: 16px; 
   height: 16px; 
   font-size: 12px; 
   color: #fff; 
   line-height: 16px; 
   text-align: center;
   background:#38d430;
   margin-top:14px;
   margin-left:12px;
   border-radius: 8px;
}
.acbox .flexWarp .add-ico .n.show{
    display: block;
}

.abbox{}
.abbox .bdbox{ background: #f5f5f5;}
.abbox .bdbox .ico{ width: 25px; height: 50px; background: url(data:image/png;base64,) no-repeat;
background-size:70%;
background-position:0px -57px;
}
.abbox .bdbox.on .ico{
   background-position:0px 16px;
}


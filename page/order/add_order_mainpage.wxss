/*order-bottom */
.order-bottom {
  height: 50px;
  background: #fff;
  display: flex;
  position: fixed;
  z-index: 100;
  bottom: 0;
  left: 0;
  right: 0;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  /* border-top: 1px solid #ccc; */
  color: #131313; font-size: 14px;-moz-box-shadow:0px -3px 20px #E3E3E3; -webkit-box-shadow:0px -3px 20px #E3E3E3; box-shadow:0px -3px 20px #E3E3E3;
}

.order-bottom .flexWarp { width: 100%;}
.order-bottom .flexWarp .fl{ width: 65%;}
.order-bottom .flexWarp .fr{ width: 35%;}
.order-bottom .flexWarp .gray{color: #aeaeae  !important;} /*a9a9a9*/
/* page/order/add_order_submit.wxss */

page {
   background-color:#f5f5f5;
   color:#131313;
   font-size:14px;
   font-family:-apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

/****** 订单订单提交页通用盒子 ******/
.orderbox{ background: #fff; padding: 10px 25px; /*min-height: 50px;*/ /*border-radius: 3px 3px 3px 3px;*/ /*margin-bottom: 10px;*/}
.orderbox .or-line{ background-color: #f3f3f3; height: 2px; font-size: 0; overflow: hidden; width: 86%; margin: 0 auto}
.orderbox .or-dbox { height: 1px; position: relative; z-index: 100; width: 100%; display: flex; width: 86%; margin: 0 auto}

.orderbox .flexWarp .lkey{ width: 30%;}
.orderbox .flexWarp .rcon{ width: 70%;} 

.bline{border-bottom: 1px solid #ebebeb;}
.acbox,.bdbox{ background: #fff; padding: 0px 25px; color: #a9a9a9;}
.abbox .acbox{ background: #f5f5f5;}
.acbox .flexWarp .tline,
.acbox .flexWarp .cline{border-bottom: 1px solid #ebebeb; width: 100%;}
.acbox .flexWarp .tline{}
.acbox .flexWarp .cline{}


/****** 布局结构 liuxinxiu by 2018-07-26 ******/

.paraWarp{
   width: 100%;
   margin-bottom: 0px !important;
}
.paraWarp .lkey{
   width: 140rpx !important;
}
.paraWarp.tline{border-bottom: 1px solid #ebebeb; width: 100%;}
.paraWarp .input{min-height: 50px !important; height: 50px !important; vertical-align: middle; display: inline-block; width: 100%;}

/****** liuxinxiu by 2018-07-26 ******/
.acbox .flexWarp{
  display:flex;/*设为伸缩容器*/  
  flex-flow:row;/*伸缩项目单行排列*/  
}
.acbox .flexWarp .fl{
   flex:1;/*这里设置为占比1，填充满剩余空间*/ 
   -webkit-box-flex: 1; 
}


/******* order-bottom *******/
.order-bottom {
  height: 50px;
  background: #fff;
  display: flex;
  position: fixed;
  z-index: 100;
  bottom: 0;
  left: 0;
  right: 0;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  /* border-top: 1px solid #ccc; */
  color: #131313; font-size: 14px;-moz-box-shadow:0px -3px 20px #E3E3E3; -webkit-box-shadow:0px -3px 20px #E3E3E3; box-shadow:0px -3px 20px #E3E3E3;
}

.order-bottom .flexWarp { width: 100%;}
.order-bottom .flexWarp .fl{ width: 65%;}
.order-bottom .flexWarp .fr{ width: 35%;}
.order-bottom .flexWarp .gray{color: #aeaeae  !important;} /*a9a9a9*/

/****** 顶部服务选择 liuxinxiu by 2018-07-26 ******/
.tabc{ height: 90px;}
.tabc .dtext{ padding-top: 50px;}
.tabc .fl,
.tabc .fr{ width: 50%; height: 90px; float: left; color: #a4a4a4; font-size: 14px;}

.tabc .fl{ background-image: url(data:image/png;base64,);
   background-repeat: no-repeat;
   background-position: center -185px;
   background-size: 14%;
 }

 .tabc .fr{
   background-repeat: no-repeat;
   background-position: center -148px;
   background-size: 18%;
   background-image: url(data:image/png;base64,)
 }
.tabc .fl.on{
   background-position: center 0px;
   color: #38d430;
}
.tabc .fr.on{
   background-position: center 0px;
   color: #38d430;
}
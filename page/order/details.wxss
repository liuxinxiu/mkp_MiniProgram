/* page/order/details.wxss */

page {
   background-color:#f3f3f3;
   color:#131313;
   font-size:14px;
   font-family:-apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

/****** 订单详情页通用盒子 ******/
.orderbox{ background: #fff; padding: 10px; /*min-height: 50px;*/ border-radius: 3px 3px 3px 3px; margin-bottom: 10px;}
.orderbox .or-line{ background-color: #f3f3f3; height: 2px; font-size: 0; overflow: hidden; width: 86%; margin: 0 auto}
.orderbox .or-dbox { height: 1px; position: relative; z-index: 100; width: 100%; display: flex; width: 86%; margin: 0 auto}

/****** 设置白色的底色 ******/
.orderbox .or-dbox .or-dot{ width: 46px; height: 20px; background-color: #fff; margin-top: -6px !important;}
.orderbox .or-dbox .or-dot._a{ margin-left: 0%; margin-right: 13%; }
.orderbox .or-dbox .or-dot._b{ margin-left: 13%; margin-right: 13%;}
.orderbox .or-dbox .or-dot._c{ margin-left: 13%; margin-right: 13%;}
.orderbox .or-dbox .or-dot._d{ margin-left: 13%; margin-right: 0%;}

/****** 设置灰色的小圆点 ******/
.orderbox .or-dbox .or-dot .or-dot-g{width: 8px; height: 8px; background-color: #ccc; margin-top: 1px !important; border-radius: 8px 8px 8px 8px; margin: 0 auto}
.orderbox .or-dbox .or-dot .greenbg{ background-color: #38d430}

/****** 设置彩色ICON(对勾_叉号) ******/
.orderbox .or-dbox .or-ico{width: 18px; height: 18px; margin-top: -10px !important; }
.orderbox .or-dbox .or-ico.on {
   background-image: url(data:image/png;base64,); background-size: 100% 100%;
}

.orderbox .or-dbox .or-ico.failed {
   background-image: url(data:image/png;base64,);
   background-size: 100% 100%;
}


.orderbox .or-dbox .or-ico._a{ margin-left: 0%; margin-right: 3%; }
.orderbox .or-dbox .or-ico._b{ margin-left: 22%; margin-right: 7%;}
.orderbox .or-dbox .or-ico._c{ margin-left: 19%; margin-right: 7%;}
.orderbox .or-dbox .or-ico._d{ margin-left: 18%; margin-right: 0%;}

/****** 设置订单状态头部的文字样式 ******/
.orderbox .or-dbox .or-text{width: 90px; height: 18px; margin-top: 11px !important; color: #a9a9a9; white-space:nowrap; font-size: 12px;}
.orderbox .or-dbox .or-text._a{ margin-left: -5%; margin-right:5%; }
.orderbox .or-dbox .or-text._b{ margin-left: 8%; margin-right: 6%;}
.orderbox .or-dbox .or-text._c{ margin-left: 8%; margin-right: 6%;}
.orderbox .or-dbox .or-text._d{ margin-left: 7%; margin-right: -6%;}

.zdd{ color: #4b4b4b}

.paraWarp{ line-height: 1.8em;}
.paraWarp .rcon{color: #a9a9a9}